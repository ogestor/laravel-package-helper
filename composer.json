        "classmap": [
            "tests/TestCase.php",
            "tests/TestServiceProvider.php"
        ]
    },
    "require": {
        "laravel/framework": "^5.6|^5.7|^5.8|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "orchestra/database": "3.1.*|3.2.*|3.3.*|3.4.*-dev",
        "orchestra/testbench": "3.1.*|3.2.*|3.3.*|3.4.*-dev",
        "mockery/mockery": "^0.9.5"
    }
}
